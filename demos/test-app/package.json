{
  "name": "@qaai-demos/test-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Test application with various UI scenarios for AI testing demos",
  "scripts": {
    "dev": "concurrently \"pnpm:dev:*\"",
    "dev:frontend": "vite",
    "dev:backend": "tsx watch server/index.ts",
    "build": "pnpm build:frontend && pnpm build:backend",
    "build:frontend": "vite build",
    "build:backend": "tsc --project server/tsconfig.json",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.8"
  }
}

